<div class="login">
<h3>LOGIN TO YOUR ACCOUNT</h3>

<%= form_tag login_path do %>
<p>
  <%= label_tag :email %>
  <%= email_field_tag :email, "", :required => true %>
</p>
<p>
  <%= label_tag :password %>
  <%= password_field_tag :password, "", :required => true %>
</p>

<%= submit_tag "Login" %>
<% end %>
</div>

<p>localhost3000/login (session - new.html.erb)</p>
